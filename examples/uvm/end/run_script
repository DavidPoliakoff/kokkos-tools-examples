KOKKOS_PROFILE_LIBRARY=/ascldap/users/dzpolia/src/kokkos-tools-examples/build/caliper/src/libcaliper.so CALI_CALIPER_ATTRIBUTE_DEFAULT_SCOPE=process CALI_LOG_VERBOSITY=1 CALI_SERVICES_ENABLE=cuptitrace:cupti:recorder:timestamp:event:kokkoslookup:alloc:aggregate:kokkostime:report CALI_REPORT_CONFIG="SELECT function,alloc.label#cupti.fault.addr,cupti.uvm.kind,inclusive_sum(sum#cupti.uvm.bytes),sum(sum#cupti.uvm.bytes) WHERE sum#cupti.uvm.bytes GROUP BY function,alloc.label#cupti.fault.addr,cupti.uvm.kind FORMAT TREE" CALI_ALLOC_RESOLVE_ADDRESSES=TRUE CALI_CUPTITRACE_ACTIVITIES=uvm CALI_CUPTITRACE_FLUSH_ON_SNAPSHOT=true $@

